delete from tblkapakkapaktip t where t.kapaketiketi ='E0100A00019' limit  1
delete from tbl10ficigovdetipeslestirme where  t.id=''     

SELECT kapaketiketi
FROM tblkapakkapaktip t
GROUP BY t.kapaketiketi   
HAVING COUNT(*) > 1 
                       





DELETE FROM tblkapakkapaktip t 
    WHERE t.id NOT IN
    (
     SELECT kapaketiketi
FROM tblkapakkapaktip t
GROUP BY t.kapaketiketi   
HAVING COUNT(*) > 1 
    );
   
